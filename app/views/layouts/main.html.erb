<html ng-app="gifts">
	<head>
		<title>Мастерская "Н"</title>
		<%= javascript_include_tag "touchTouch.jquery.js" %>
		<%= javascript_include_tag "jquery-1.7.min.js" %>
		<%= javascript_include_tag "jquery.featureCarousel.js" %>
		<%= javascript_include_tag "application" %>
		<script src="http://angular-ui.github.io/bootstrap/ui-bootstrap-tpls-0.11.0.min.js"></script>
		<%= stylesheet_link_tag 'index.css.scss' %>
		<%= stylesheet_link_tag 'touchTouch.css' %>
		<%= stylesheet_link_tag 'feature-carousel.css' %>
		<%= stylesheet_link_tag "http://fonts.googleapis.com/css?family=PT+Sans&subset=latin,cyrillic" %>
		<script type="text/javascript" src="//vk.com/js/api/openapi.js?98"></script>
		<script>
		  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
		  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
		  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
		  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

		  ga('create', 'UA-43297503-1', 'studio-n.by');
		  ga('send', 'pageview');
		</script>
		</style>
	</head>
<body>
	<table style="width:100%;height:100%;border-collapse:collapse;table-layout:fixed" cellpadding="0" cellspacing="0">
		
		<tbody>
			<tr>
				<td>
					<div id="left-bg"></div>
				</td>
				<td width="34px">
					<div id="left-bg-border"></div>
				</td>
				<td style="width:1000px;padding:0px;vertical-align: top;">
					<div class="top">
						<header><%= render 'index/header' %></header>
						<div id="menu"><%= render 'index/navi' %></div>
						<div id="content"><%= yield %></div>
						<footer><%= render 'index/footer' %></footer>
					</div>
				</td>
				<td width="34px">
					<div id="right-bg-border"></div>
				</td>
				<td>
					<div id="right-bg"></div>
				</td>
			</tr>
		</tbody>
	</table>
</body>
</html>